  "scripts": {
    "comment0": "run these with: npm run <command>",
    "ng": "ng",
    "comment1": "Added publichost flag to npm start cause zone.js/sockjs-node error was appearing in browser console",
    "start": "ng serve --public-host=localhost:4200 --open",
    "build:dev": "ng build  --source-map",
    "comment": "the production arg for ng build is defined in angular.json",
    "build:prod": "ng build --configuration production --optimization true",
    "comment3": "these ng test, ng lint , ng e2e commands are defined in angular.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "comment2": "Below command compiles sass into css. See Angular.json file",
    "scss": "sass scss/custom.scss src/css-from-sass/custom.css"
  },
